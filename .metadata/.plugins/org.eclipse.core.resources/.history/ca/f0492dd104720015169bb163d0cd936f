package level_0;

public class potatos {
	public static void main(String[] args) {
		String s = "";
		for(int i=0; i<1; i++)
		{
			for(int I=0; I<8; I++){
				if(I == 1){s = "";}
				else{s = "s";}
				if(I != 7){
					System.out.println(I + "potato");
				}
				else{
					System.out.println("more");
				}
				
			
		}
		}

}
}
